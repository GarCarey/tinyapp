<header>
  <nav class="navbar navbar-expand-md nabar-dark bg-success">
    <a class="navbar-brand" href="/urls" style="color: white;">TinyApp</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-targets="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav=item nav-link" href="/urls" style="color: white;">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new" style="color: white;">Create New URL</a>
      </div>
    </div>
    <% if (user) { %>
      <h4>Logged in as: <%= user["email"] %>&emsp;</h4>
      <form class="form-inline" action="/logout" method="POST">
        <div class="form-group mb-2">
              <button type="submit" class="btn btn-primary">Log Out</button>
        </div>
      </form>
    <% } else { %>
        <div class="form-inline">
          <form action="/register" method="GET">
            <button class="btn btn-primary" href="/register">Register</button>
          </form> 
          <div class="divider">&ensp;</div>
          <form action="/login" method="GET"> 
            <button class="btn btn-primary" href="/login">Log In</button>
          </form>
        </div>
    <% } %>
  </nav>
</header>